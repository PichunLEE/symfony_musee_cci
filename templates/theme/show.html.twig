{% extends 'base.html.twig' %}

{% block title %}Theme{% endblock %}

{% block contents %}
    <h1>Theme</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ theme.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ theme.label }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ theme.description }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
